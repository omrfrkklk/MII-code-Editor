<mvc:View
	controllerName="app.controller.SideNav"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:tnt="sap.tnt"
	height="100%"
	class="side-navigation-demo">

		<Tree
		    id="Tree"
			includeItemInSelection="true"
			selectionChange=".onItemSelectionChange"
		    items="{path: '/'}">
			<StandardTreeItem title="{text}"/>
		</Tree>
</mvc:View>
