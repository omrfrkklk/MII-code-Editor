<mvc:View
	controllerName="app.controller.CodeEditor"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:ce="sap.ui.codeeditor"
	height="100%">
	<IconTabHeader
		id="iconTabHeader"
		selectedKey="invalidKey"
		select=".onSelectTab">
		<items>
			<IconTabFilter text="javaScript" key="js" />
			<IconTabFilter text="css" key="css" />
			<IconTabFilter text="xml" key="xml" />
		</items>
	</IconTabHeader>
	<ce:CodeEditor id="aCodeEditor" width="1100px" height="700px" type="javascript" />
</mvc:View>
